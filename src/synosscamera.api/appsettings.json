{

  "AllowedHosts": "*",

  "SynossSettings": {
    "ApiKeys": [
      {
        "Name": "sample_api_key",
        "ApiKey": "api123token234",
        "Enabled": true
      },
      {
        "Name": "hook_api_key",
        "ApiKey": "keyforhooks123",
        "Enabled": true
      }
    ]
  },

  "SurveillanceStationSettings": {
    "BaseUrl": "url to webapi",
    "SessionNameForStation": "synosscamera",
    "Username": "setusername",
    "Password": "userspassword"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Logicx": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./wwwroot/data/debug.log",
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 10485760,
          "retainedFileCountLimit": 3,
          "rollOnFileSizeLimit": true,
          "shared": true,
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./wwwroot/data/error.log",
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 10485760,
          "retainedFileCountLimit": 5,
          "rollOnFileSizeLimit": true,
          "shared": true,
          "restrictedToMinimumLevel": "Error"
        }
      }
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
